{{#each stations}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
        {{> stationsummary station=this}}
    <a href="/station/{{id}}" class="ui icon button">
      <i class="purple large icon folder open"></i>
    </a>
    <a href="/dashboard/deletestation/{{id}}" class="ui icon button">
      <i class="red large icon trash"></i>
    </a>
  </section>
{{/each}}